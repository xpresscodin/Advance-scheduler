{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "handle": "filesystem" },
    { "src": "/availability", "dest": "/availability.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
